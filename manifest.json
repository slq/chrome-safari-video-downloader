{
  "name": "Slawek",
  "description": "Slawek PL",
  "version": "1.8",
  "permissions": [
    "cookies",
    "downloads",
    "*://*.safaribooksonline.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.safaribooksonline.com/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "core/util.js",
        "core/shared.js",
        "core/techbus.js",
        "core/new.js",
        "main.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "manifest_version": 2,
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "background": {
    "scripts": [
      "core/background.js"
    ]
  },
  "web_accessible_resources": [
    "icon/48.png"
  ]
}
